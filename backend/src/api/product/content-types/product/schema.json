{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "preis": {
      "type": "decimal"
    },
    "bild": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "hersteller": {
      "type": "string"
    },
    "beschreibung": {
      "type": "richtext"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "products"
    },
    "onSale": {
      "type": "boolean",
      "default": false
    },
    "salePrice": {
      "type": "float"
    }
  }
}
